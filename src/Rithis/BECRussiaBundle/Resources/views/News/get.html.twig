{% extends "RithisBECRussiaBundle::base.html.twig" %}

{% block content %}
    <div class="clearfix">
        <h1>{{ news.title }} <small>{{ news.createdAt|date('d.m.Y') }}</small></h1>
        <hr>
        {% thumbnail news.image, "big" with {class: "pull-left"} %}
        <div>{{ news.description|raw }}</div>
    </div>
{% endblock %}
